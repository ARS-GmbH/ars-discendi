<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE FindBugsFilter>
<FindBugsFilter>
    <Match>
        <Class name="~.*\$.*" />
    </Match>
    <Match>
        <Bug code="CD" />
        <Class name="~.*" />
    </Match>
    <Match>
        <Class name="~.*Messages" />
    </Match>
    <Match>
        <Bug code="UwF, SIC, NP, Dm" />
        <Class name="~.*Test.*" />
    </Match>
    <Match>
        <Bug code="UwF" />
        <Class name="~.*View.*" />
    </Match>
</FindBugsFilter>
